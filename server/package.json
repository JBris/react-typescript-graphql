    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "apollo-server": "^2.10.1",
    "got": "^10.5.7",
    "graphql": "^14.6.0",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6"
  }
